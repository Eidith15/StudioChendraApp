<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eidith.studiochendraapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="30" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="androidx.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.StudioChendraApp" >
        <activity android:name="com.eidith.studiochendraapp.activity.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.eidith.studiochendraapp.activity.ArtikelActivity"
            android:parentActivityName="com.eidith.studiochendraapp.activity.MainActivity" />
        <activity
            android:name="com.eidith.studiochendraapp.activity.DetailArtikelActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:parentActivityName="com.eidith.studiochendraapp.activity.ArtikelActivity" />
        <activity
            android:name="com.eidith.studiochendraapp.activity.TambahArtikelActivity"
            android:parentActivityName="com.eidith.studiochendraapp.activity.ArtikelActivity" />
        <activity
            android:name="com.eidith.studiochendraapp.activity.WorkshopActivity"
            android:parentActivityName="com.eidith.studiochendraapp.activity.MainActivity" />
        <activity
            android:name="com.eidith.studiochendraapp.activity.DetailWorkshopActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:parentActivityName="com.eidith.studiochendraapp.activity.WorkshopActivity" />
        <activity
            android:name="com.eidith.studiochendraapp.activity.TambahWorkshopActivity"
            android:parentActivityName="com.eidith.studiochendraapp.activity.WorkshopActivity" />
        <activity
            android:name="com.eidith.studiochendraapp.activity.LayananActivity"
            android:parentActivityName="com.eidith.studiochendraapp.activity.MainActivity" />
        <activity
            android:name="com.eidith.studiochendraapp.activity.DetailLayananActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:parentActivityName="com.eidith.studiochendraapp.activity.LayananActivity" />
        <activity
            android:name="com.eidith.studiochendraapp.activity.TambahLayananActivity"
            android:parentActivityName="com.eidith.studiochendraapp.activity.LayananActivity" />
        <activity
            android:name="com.eidith.studiochendraapp.activity.PortofolioActivity"
            android:parentActivityName="com.eidith.studiochendraapp.activity.MainActivity" />
        <activity
            android:name="com.eidith.studiochendraapp.activity.DetailPortofolioActivity"
            android:parentActivityName="com.eidith.studiochendraapp.activity.PortofolioActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="com.eidith.studiochendraapp.activity.TambahPortofolioActivity"
            android:parentActivityName="com.eidith.studiochendraapp.activity.PortofolioActivity" />
    </application>

</manifest>